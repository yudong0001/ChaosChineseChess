<!DOCTYPE html>
<%
var cellSize = 50;
%>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <% include js/index_js %>
  </head>
  <body>
    <!-- <h1><%= title %></h1> -->
    <p>欢迎赏玩<%= title %></p>
	<div id="board">
	<% for (var i = 0; i < rowsCount; i++) { %>
		<% for (var j = 0; j < columnsCount; j++) { %>
      <div id="cell<%= i %><%= j %>" class="cell" style="width:<%=cellSize%>px;height:<%=cellSize%>px;top:<%=i*cellSize%>px;left:<%=j*cellSize%>px;" >                
			</div>
		<% } %>
		<br>
	<% } %>
	</div>
  </body>
</html>
